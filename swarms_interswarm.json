[
    {
        "name": "swarm-alpha",
        "version": "1.0.0",
        "description": "Alpha swarm for interswarm communication example",
        "base_url": "http://localhost:8000",
        "agents": [
            {
                "name": "supervisor",
                "factory": "acp.factories.supervisor:supervisor_factory",
                "llm": "openai/o3-mini",
                "system": "acp.examples.supervisor.prompts:SYSPROMPT",
                "comm_targets": ["weather", "math", "external-consultant"],
                "agent_params": { 
                    "enable_interswarm": true
                }
            },
            {
                "name": "weather",
                "factory": "acp.examples.weather_dummy.agent:factory_weather_dummy",
                "llm": "openai/o3-mini",
                "system": "acp.examples.weather_dummy.prompts:SYSPROMPT",
                "comm_targets": ["supervisor", "math"],
                "agent_params": { }
            },
            {
                "name": "math",
                "factory": "acp.examples.math_dummy.agent:factory_math_dummy",
                "llm": "openai/o3-mini",
                "system": "acp.examples.math_dummy.prompts:SYSPROMPT",
                "comm_targets": ["supervisor", "weather"],
                "agent_params": { }
            }
        ]
    },
    {
        "name": "swarm-beta",
        "version": "1.0.0",
        "description": "Beta swarm for interswarm communication example",
        "base_url": "http://localhost:8001",
        "agents": [
            {
                "name": "supervisor",
                "factory": "acp.factories.supervisor:supervisor_factory",
                "llm": "openai/o3-mini",
                "system": "acp.examples.supervisor.prompts:SYSPROMPT",
                "comm_targets": ["consultant", "analyst"],
                "agent_params": { 
                    "enable_interswarm": true
                }
            },
            {
                "name": "consultant",
                "factory": "acp.examples.consultant_dummy.agent:factory_consultant_dummy",
                "llm": "openai/o3-mini",
                "system": "acp.examples.consultant_dummy.prompts:SYSPROMPT",
                "comm_targets": ["supervisor", "analyst"],
                "agent_params": { }
            },
            {
                "name": "analyst",
                "factory": "acp.examples.analyst_dummy.agent:factory_analyst_dummy",
                "llm": "openai/o3-mini",
                "system": "acp.examples.analyst_dummy.prompts:SYSPROMPT",
                "comm_targets": ["supervisor", "consultant"],
                "agent_params": { }
            }
        ]
    }
]
